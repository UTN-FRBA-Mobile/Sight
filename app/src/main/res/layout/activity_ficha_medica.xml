<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_fichamedica"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_fichamedica"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="rsa.sight.FichaMedicaActivity">






            <TextView
                android:id="@+id/bienvenida"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:text="Bienvenida" />


            <Button
                android:id="@+id/volver"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:onClick="volver"
                android:text="Volver" />

            <EditText
                android:id="@+id/listaPadecimientos"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:inputType="text|textMultiLine"
                android:gravity="top"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="282dp"
                android:layout_below="@+id/textView"
                android:layout_marginLeft="22dp" />




            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="304dp"
                android:text="Enfermedades"
                android:textSize="24sp"
                android:textStyle="bold" />






    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="31dp"
        android:layout_marginLeft="50dp"
        >

        <ImageButton
            android:id="@+id/botonPerfil"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:background="@android:color/transparent"
            android:layout_marginRight="50dp"
            android:clickable="true"
            android:onClick="perfil"
            app:srcCompat="@drawable/menuprincipal_perfil" />

        <ImageButton
            android:id="@+id/botonInicio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginRight="50dp"
            android:layout_toRightOf="@+id/botonPerfil"
            android:background="@android:color/transparent"
            android:clickable="true"
            android:onClick="inicio"
            app:srcCompat="@drawable/menuprincipal_inicio" />


        <ImageButton
            android:id="@+id/botonNotif"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@+id/botonInicio"
            android:background="@android:color/transparent"
            android:clickable="false"
            app:srcCompat="@drawable/menuprincipal_notificaciones" />


    </RelativeLayout>









</RelativeLayout>
